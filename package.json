{
  "name": "flow-insight",
  "productName": "FlowInsight",
  "description": "FlowInsight - © 2023 Twilight City, Inc. All rights reserved.",
  "version": "0.7.12",
  "engines": {
    "node": "16.13.0"
  },
  "private": true,
  "author": {
    "name": "Twilight City, Inc.",
    "email": "admin@twilightcity.net"
  },
  "copyright": "Copyright © 2023 - Twilight City, Inc.",
  "homepage": "./",
  "main": "public/electron.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/twilightcity-net/flowinsight-release.git"
  },
  "scripts": {
    ".": "clear && yarn dev",
    "..": "clear && yarn docs && yarn stage && yarn dev",
    "...": "yarn release",
    ",": "yarn stage",
    ",,": "yarn docs",
    "/": "yarn dev:grid",
    "doctor": "echo take the purple pills",
    "update": "git fetch && git pull && yarn prestage",
    "prestage": "gulp prettier lint pretty",
    "stage": "git add .",
    "start": "react-scripts start",
    "dev:deactivate": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron . RENDER3D=false --DEACTIVATE\"",
    "dev:local": "concurrently \"yarn server\" \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron . SERVER=http://localhost:5000 TALK=http://localhost:5050 RENDER3D=false\"",
    "dev:grid": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron . SERVER=http://localhost:8080 RENDER3D=false\"",
    "dev:talk": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron . TALK=http://localhost:5000 RENDER3D=false\"",
    "dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron . RENDER3D=false\"",
    "server": "node server/Server.js",
    "docs": "jsdoc public/electron.js --configure .jsdoc.json --verbose",
    "test": "cross-env ENV=test mocha test/SetupApi.js test/**/*Test.js --exit",
    "test:hyper": "cross-env ENV=test mocha test/SetupHyper.js test/hyper/**/*Test.js",
    "test:talk": "node test/talk/RTTest.js",
    "test:app": "cross-env ENV=test electron-mocha test/SetupApp.js test/app/**/*Test.js",
    "prebuild": "gulp prettier lint pretty",
    "build": "react-scripts build",
    "postbuild": "yarn docs",
    "prebundle": "yarn build",
    "bundle": "gulp browserify && gulp uglify",
    "postbundle": "gulp clean",
    "predist": "yarn bundle",
    "prerelease": "yarn bundle",
    "dist": "electron-builder -c.extraMetadata.main=build/net.twilightcity.flowinsight.dat --publish=never",
    "release": "electron-builder -c.extraMetadata.main=build/net.twilightcity.flowinsight.dat --publish=always",
    "prerelease:build:update": "yarn prerelease",
    "release:build:update": "gulp releaseBuildUpdate",
    "release:build:update:run": "gulp releaseBuildUpdate && npm run dev"
  },
  "dependencies": {
    "@keyframes/core": "^2.0.10",
    "@types/react": "^17.0.33",
    "asar": "^3.1.0",
    "chalk": "^4.1.2",
    "chalk-animation": "^1.6.0",
    "clean-stack": "^1.3.0",
    "crypto-js": "^4.1.1",
    "d3": "^7.3.0",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^2.2.6",
    "electron-platform": "^1.2.0",
    "electron-root-path": "^1.0.16",
    "electron-settings": "^3.2.0",
    "fitty": "2.3.6",
    "lokijs": "^1.5.12",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "mousetrap": "^1.6.5",
    "p5": "1.4.1",
    "query-string": "7.1.1",
    "react": "17.0.2",
    "react-colorful": "^5.5.0",
    "react-console-emulator": "^5.0.1",
    "react-dom": "17.0.2",
    "react-router-dom": "^5.3.0",
    "react-splitter-layout": "^4.0.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.4",
    "socket.io-client": "^2.4.0",
    "stack-trace": "^0.0.10",
    "superagent": "7.1.1",
    "unicode-emoji": "2.3.0",
    "yargs": "^14.0.0"
  },
  "devDependencies": {
    "body-parser": "^1.19.0",
    "browserify": "^17.0.0",
    "chai": "^4.3.4",
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.3",
    "devtron": "^1.4.0",
    "docdash": "^1.2.0",
    "electron": "^17.4.0",
    "electron-builder": "^23.0.2",
    "electron-mocha": "^10.1.0",
    "express": "^4.17.1",
    "fs-extra": "^10.0.0",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.4.0",
    "gulp-gnirts": "^1.0.6",
    "gulp-jshint": "^2.1.0",
    "gulp-prettier-plugin": "^1.3.0",
    "gulp-pretty-html": "^2.0.10",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-terser": "2.1.0",
    "jsdoc": "^3.6.7",
    "jshint": "^2.13.1",
    "jshint-stylish": "^2.2.1",
    "mocha": "^9.1.3",
    "prettier": "^2.4.1",
    "react-error-overlay": "^6.0.9",
    "react-scripts": "^3.4.4",
    "terser": "5.11.0",
    "uglify-js": "^3.15.2",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "wait-on": "^6.0.0"
  },
  "resolutions": {
    "graceful-fs": "^4.2.4",
    "react-error-overlay": "6.0.9",
    "socket.io-client": "^2.4.0",
    "chokidar": "^3.5.3",
    "highlight.js": "^11.5.1",
    "lodash": "^4.17.21",
    "immer": "^9.0.6",
    "async": "^2.6.4",
    "node-forge": "^1.3.0",
    "ansi-html": "0.0.8",
    "trim-newlines": "^3.0.1",
    "glob-parent": "^5.1.2",
    "lodash.template": "^4.5.0",
    "nanoid": "^3.3.1",
    "browserslist": "^4.20.2",
    "nth-check": "^2.0.1",
    "node-notifier": "^8.0.1"
  },
  "jshint": {
    "esversion": 6,
    "lookup": false
  },
  "prettier": {
    "configJS": {
      "printWidth": 60,
      "tabWidth": 2,
      "bracketSpacing": true
    },
    "configJSON": {
      "parser": "json",
      "printWidth": 60,
      "tabWidth": 2,
      "bracketSpacing": true
    }
  },
  "browserify": {
    "entries": "build/electron.js",
    "builtins": false,
    "browserField": false,
    "commondir": true,
    "ignoreMissing": true,
    "insertGlobalVars": "global",
    "bundleExternal": false
  },
  "uglify": {
    "parse": {
      "html5_comments": false,
      "shebang": false
    },
    "compress": {
      "dead_code": true,
      "drop_debugger": true,
      "booleans": true,
      "loops": true,
      "unused": true,
      "toplevel": true,
      "top_retain": true,
      "hoist_vars": true,
      "if_return": true,
      "inline": true,
      "join_vars": true,
      "reduce_vars": true,
      "warnings": true,
      "drop_console": true,
      "passes": 3,
      "ecma": 6
    },
    "mangle": {
      "toplevel": true,
      "eval": true
    },
    "output": {
      "preamble": "/* Copyright © 2023 Twilight City, Inc // FlowInsight */",
      "bracketize": true,
      "ecma": 6,
      "indent_level": 0,
      "max_line_len": false,
      "preserve_line": false,
      "quote_keys": false
    },
    "ecma": 6,
    "nameCache": null,
    "ie8": false,
    "warnings": true
  },
  "build": {
    "appId": "net.twilightcity.flowinsight",
    "files": [
      "build/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "win": {
      "target": "nsis"
    },
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "nsis": {
      "perMachine": true,
      "artifactName": "${productName}-Setup-${version}.${ext}",
      "uninstallDisplayName": "${productName}",
      "differentialPackage": false,
      "warningsAsErrors": false
    },
    "publish": {
      "provider": "github"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
